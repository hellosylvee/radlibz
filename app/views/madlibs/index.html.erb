<h1>Select a Radlib!</h1><br></br>

<ul>
  <div class="row">
    <div class="col s12">
    <% @madlibs.each do |madlib| %>
      <li>
        <h5>Title: <%= link_to madlib.title, new_madlib_user_vocab_path(madlib) %></h5>
        <p>Description: <%= madlib.description %></p>
      </li><br>
      <div class="divider"></div>
    <% end %>
    </div>
  </div>
</ul>
